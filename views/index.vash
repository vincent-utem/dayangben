@html.extend('layouts/main', function(model){

    @html.block('head', function(headmodel){
        <h2>head</h2>
    })

    @html.block('body', function(bodymodel){
        <h1>@model.title</h1>
        @html.include('partial', model)
    })

    @html.block('bottom', function(bottommodel){
        <h3>bottom</h3>
    })

})